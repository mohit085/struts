<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
          "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
          "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

	<package name="student-app" extends="struts-default">
		<action name="studform"
			class="com.example.action.StudentAction"
			method="showStudForm">
			<result name="none">/WEB-INF/content/studentform.jsp</result>
			</action>
			
			<action name="register"
				class="com.example.action.StudentAction"
				method="showStudForm">
				<result name="none">/WEB-INF/content/studform.jsp</result>
			</action>
	</package>
</struts>